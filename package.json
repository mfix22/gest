{
  "name": "graphicli",
  "version": "1.4.0",
  "description": "GraphQL testing CLI",
  "main": "./src/index.js",
  "engines": {
    "node": ">= 6.9.0"
  },
  "dependencies": {
    "args": "^2.3.0",
    "axios": "^0.15.3",
    "chalk": "^1.1.3",
    "graphql": "^0.9.1"
  },
  "devDependencies": {
    "eslint": "^3.17.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "jest": "^19.0.2"
  },
  "scripts": {
    "lint": "eslint src bin test",
    "test": "npm run lint && NODE_ENV=test jest",
    "coverage": "npm run test -- --coverage --silent",
    "start": "node ./bin/gest"
  },
  "author": "Michael Fix <@mfix22>",
  "license": "MIT",
  "bin": {
    "gest": "./bin/gest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfix22/graphicli.git"
  },
  "keywords": [
    "cli",
    "graphql",
    "graphiql",
    "gest",
    "test",
    "testing",
    "graphicli"
  ],
  "bugs": {
    "url": "https://github.com/mfix22/graphicli/issues"
  },
  "homepage": "https://github.com/mfix22/graphicli#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ]
  },
  "gest": {
    "schema": "./test/_schema.js"
  }
}
