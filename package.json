{
  "name": "gest",
  "version": "1.6.0",
  "description": "GraphQL testing CLI",
  "main": "./src/index.js",
  "engines": {
    "node": ">= 6.9.0"
  },
  "dependencies": {
    "apollo-link-http": "^1.5.14",
    "args": "^5.0.1",
    "chalk": "^2.1.0",
    "node-fetch": "^2.6.0",
    "ora": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
